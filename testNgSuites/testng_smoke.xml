<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite 1">
    <!--    only run smoke as this is priority over others-->
    <groups>
        <run>
            <include name="smoke"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="org.example.testutils.ListenersNg"/>
    </listeners>
    <test verbose="2" name="e2e framework test">
        <classes>
            <class name="org.example.AndroidBasics_Ecommerce"/>
        </classes>
    </test>

</suite>